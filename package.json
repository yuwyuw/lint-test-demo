{
  "name": "lint-demo",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "husky",
    "lint": "eslint src"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js, .eslintignore": [
      "eslint --fix",
      "git add",
      "prettier --write"
    ],
    "*.ts, .eslintignore": [
      "eslint --fix",
      "git add",
      "prettier --write"
    ],
    "*.tsx, .eslintignore": [
      "eslint --fix",
      "git add",
      "prettier --write"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@eslint/compat": "^1.0.1",
    "@eslint/js": "^9.3.0",
    "eslint": "9.x",
    "eslint-plugin-react": "^7.34.1",
    "globals": "^15.3.0",
    "husky": "^9.0.11",
    "prettier": "^3.2.5",
    "typescript-eslint": "^7.10.0"
  }
}
